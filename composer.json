{
    "name": "phpactor/code-transform",
    "description": "Collection of classes which transform, generate and refactor code",
    "license": "MIT",
    "type": "phpactor-extension",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],

    "require": {
        "phpactor/container": "^1.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.10.5",
        "phpunit/phpunit": "^7.4",
        "friendsofphp/php-cs-fixer": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\Extension\\CodeTransform\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\Extension\\CodeTransform\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpactor.extension_class": "Phpactor\\Extension\\CodeTransform\\CodeTransformExtension",
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
